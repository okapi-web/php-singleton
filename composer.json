{
    "name": "okapi/singleton",
    "description": "PHP Singleton is a PHP library that allows you to create singleton classes with a trait.",
    "version": "1.0.2",
    "type": "library",
    "homepage": "https://github.com/okapi-web/php-singleton",
    "license": "MIT",
    "authors": [
        {
            "name": "WalterWoshid",
            "email": "wotschel.valentin@googlemail.com",
            "homepage": "https://github.com/WalterWoshid"
        }
    ],
    "keywords": [
        "singleton",
        "trait",
        "php"
    ],
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html tests/coverage"
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "Okapi\\Singleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okapi\\Singleton\\Tests\\": "tests/"
        }
    }
}
